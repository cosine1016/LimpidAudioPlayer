<local:ListItem x:Class="ClearUC.ListViewItems.LabelSeparator"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:ClearUC.ListViewItems"
             mc:Ignorable="d" Height="35.225" Width="342">
    <Grid>
        <Rectangle Fill="#33000000" Stroke="#4C383838" />
        <Label x:Name="label" Content="Label" Foreground="#FFC8C8C8" FontSize="18.667" FontWeight="Thin" VerticalContentAlignment="Center" Margin="10,0,0,0" />
        <Rectangle x:Name="sticky" Fill="#FF00FF64" HorizontalAlignment="Left" Stroke="Black" Width="8" Opacity="0.5" StrokeThickness="0" />
    </Grid>
</local:ListItem>